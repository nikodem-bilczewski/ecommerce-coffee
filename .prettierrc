{
  "semi": false,
  "tabWidth": 2,
  "printWidth": 80,
  "importOrder": [
    "^services(.*)$",
    "^components/(.*)$",
    "^assets/(.*)$",
    "^[./]",
    "^index.css"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "singleQuote": true,
  "trailingComma": "all",
  "jsxSingleQuote": true,
  "bracketSpacing": true
}
